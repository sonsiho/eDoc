{
	"appId": "com.example.viettel.edoc",
	"appName": "eDoc",
	"webDir": "www",
	"ios": {
		"scheme": "App"
	},
	"plugins": {
		"CapacitorSQLite": {
			"iosDatabaseLocation": "Library/CapacitorDatabase"
		},
		"Keyboard": {
			"resize": "native",
			"style": "DARK",
			"resizeOnFullScreen": true
		},
		"PushNotifications": {
			"presentationOptions": ["badge", "sound", "alert"]
		  }
	  
	}
}